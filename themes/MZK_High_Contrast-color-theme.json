{
  "name": "MZK-HC",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#ffffff",
    "editor.selectionBackground": "#FFFF00",
    "editor.selectionForeground": "#000000",
    "editor.lineHighlightBackground": "#0000ff",
    "editorCursor.foreground": "#ffffff",
    "editorWhitespace.foreground": "#0000ff",
    "tab.activeBackground": "#00ffff",
    "tab.activeForeground": "#000000",
    "tab.inactiveBackground": "#000000",
    "panel.background": "#000000",
    "activityBar.background": "#000000",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#FFFFFF",
    "gitDecoration.addedResourceForeground": "#00FF00",
    "gitDecoration.deletedResourceForeground": "#FF0000",
    "gitDecoration.conflictingResourceForeground": "#ff00FF",
    "gitDecoration.ignoredResourceForeground": "#6B6B6B",
    "gitDecoration.modifiedResourceForeground": "#FFFF00",
    "gitDecoration.submoduleResourceForeground": "#0FF",
    "gitDecoration.untrackedResourceForeground": "#0F0",
  },
  "tokenColors": [
    {
      "name": "Markup title",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#00FF00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0FF"
      }
    },

    {
      "name": "Markup italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },

    {
      "name": "Markup quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6B6B6B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#6b6b6b"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF0000"
      }
    },{
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#FFFFFF",
      }
    }
  ]
}
